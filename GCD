#include<iostream>
#include<algorithm>
#include<stdio.h>
#include<string.h>
#include <cstdio>
#include <cstring>
#include <queue>
#include <stack>
#include<vector>
#include <cmath>
using namespace std;

#define INF 0x3f3f3f3f
#define maxn 120

long long int x,y;

int gcd(int a,int b)
{
        if(b==0) return a;
        else return gcd(b,a%b) ;
}
int main()
{
     while(cin>>x>>y)
        cout<<gcd(x,y)<<endl;
     return 0;
}
